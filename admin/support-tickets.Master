                                color: var(--white);
                                border-color: var(--primary-blue);
                            }

                        .page-item.active .page-link {
                            background: var(--primary-blue);
                            border-color: var(--primary-blue);
                        }

                        .ticket-message {
                            max-width: 300px;
                            overflow: hidden;
                            text-overflow: ellipsis;
                            white-space: nowrap;
                        }

                        .assignee-info {
                            display: flex;
                            align-items: center;
                        }

                        .assignee-avatar {
