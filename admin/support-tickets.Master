                            .action-buttons {
                                flex-direction: column;
                            }

                            .table-responsive {
                                font-size: 0.9rem;
                            }

                            .ticket-info {
                                flex-direction: column;
                                text-align: center;
                            }

                            .ticket-avatar {
                                margin-right: 0;
                                margin-bottom: 0.5rem;
                            }
